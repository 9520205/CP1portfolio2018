void drawRef() {
  line(70, 141, width-455, 141);
  for (int i=70; i<width-450; i+=37) {
    textSize(10);
    text((i-70)/37, i-2, 163);
    line(i, 131, i, 151);
  }
}

void colorFirst() {
  if (mouseX<109) {
    d=0;
    e=0;
    f=0;
