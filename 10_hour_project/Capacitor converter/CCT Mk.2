    d=255;
    e=0;
    f=0;
    //red
  } else if (mouseX>182 && mouseX<220) {
    d=255;
    e=125;
    f=0;
    //orange
  } else if (mouseX>219 && mouseX<257) {
    d=255;
    e=255;
    f=0;
    //yellow
  } else if (mouseX>256 && mouseX<294) {
    d=0;
    e=160;
    f=0;
    //green
  } else if (mouseX>293 && mouseX<331) {
    d=0;
    e=50;
    f=200;
    //blue
  } else if (mouseX>330 && mouseX<368) {
    d=150;
    e=100;
    f=255;
    //violet
  } else if (mouseX>367 && mouseX<405) {
    d=200;
    e=200;
    f=200;
    //gray
